{
    "name": "dew/cli",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Li Zhineng",
            "email": "im@zhineng.li"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-zip": "*",
        "ext-posix": "*",
        "symfony/console": "^6.0|^7.0",
        "symfony/filesystem": "^6.0|^7.0",
        "symfony/finder": "^6.0|^7.0",
        "symfony/process": "^6.0|^7.0",
        "guzzlehttp/guzzle": "^7.7",
        "symfony/yaml": "^6.0|^7.0",
        "illuminate/collections": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "composer/semver": "^3.4"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "pestphp/pest": "^4.1",
        "phpstan/phpstan": "^1.12",
        "phpstan/phpstan-mockery": "^1.1",
        "rector/rector": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Dew\\Cli\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dew\\Cli\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "refactor": "rector",
        "lint": "phpstan analyse",
        "test": "pest"
    },
    "bin": ["dew"],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "sort-packages": true
    }
}
